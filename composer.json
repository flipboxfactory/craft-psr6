{
  "name": "flipboxfactory/craft-psr6",
  "description": "PSR-6 Cache for Craft CMS",
  "version": "1.0.0-rc",
  "keywords": [
    "psr-6",
    "psr6",
    "craft",
    "craft cms",
    "yii2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "flipboxdigital/stash-pool": "^1.0",
    "flipboxfactory/craft-psr3": "~1.0.0-rc",
    "craftcms/cms": "~3.0.0-RC"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\craft\\psr6\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\craft\\psr6\\tests\\": "tests/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ]
}